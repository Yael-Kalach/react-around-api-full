{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle":
      ["error", {"allow": ["_id"]}],
    "no-param-reassign":
      ["error", { "props": false }],
    "no-unused-vars":
      ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "object-shorthand":
      ["error", "always", { "ignoreConstructors": true }]
  }
}